<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="farm.gecdevelopers.com.farm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.Login" />
        <activity android:name=".activity.admin.CreateAccount" />
        <activity android:name=".activity.admin.DashBoardActivity" />
        <activity android:name=".activity.manager.Manager_DashBoardActivity" />
        <activity android:name=".activity.admin.ItemTypesActivity" />
        <activity android:name=".activity.SplashActivity"></activity>
        <activity android:name=".activity.admin.AddFarmActivity"></activity>
        <activity android:name=".activity.admin.AddItems">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.admin.AddPlot"></activity>
    </application>

</manifest>